(defvar
  tap-timeout 100
  hold-timeout 150
  tt $tap-timeout
  ht $hold-timeout

  slow-tap-timeout 150
  slow-hold-timeout 275
  stt $slow-tap-timeout
  sht $slow-hold-timeout

  one-shot-timer 500
  ost $one-shot-timer

  delay-fix 15
  df $delay-fix
)

;; Since Windows' Start menu shortcut can't be easily changed we use Kanata for it:
;; Set PowerToys' Run to Win+Spc and avoid the awful Start menu.
(platform (win winiov2 wintercept)
  ;; Tap for M-spc, hold for lmet:
  (defalias lmet (tap-hold-press 0 $ht M-spc lmet))
)
;; Meta is left alone on Linux/Mac but must be defined:
(platform (linux macos)
  (defalias lmet lmet)
)

(defalias ;; layer switches
  ;; Tap for OSL to workspaces, hold for control:
  wsctl (tap-hold-press 0 $ht (one-shot $ost (layer-while-held workspaces)) lctl)
  ;; Tap for OSL to fnsym, hold for numsym:
  nmsm (tap-hold-press 0 $ht (one-shot $ost (layer-while-held fnsym)) (layer-while-held numsym))
  ;; Tap for esacpe, hold for extend layer.
  escext (tap-hold-press 0 $ht esc (layer-while-held extend))
  base (tap-hold 0 1000 XX (layer-switch base))
  game (tap-hold 0 1000 XX (layer-switch game))
  'base (tap-hold 250 1000 ' (layer-switch base))
  'game (tap-hold 250 1000 ' (layer-switch game))
)

(defalias ;; tap-holds
  delrsft (tap-hold-press $tt $ht del rsft)
  bspclsft (fork (tap-hold-press $tt $ht bspc lsft) @delrsft (f24))

  zalt (tap-hold-press 0 $sht z ralt)
  /alt (tap-hold-press 0 $sht / ralt)
  !alt (tap-hold 0 $ht S-1 ralt)
  ?alt (tap-hold 0 $ht S-/ ralt)

  tabhyper (tap-hold-press $tt $ht tab (multi lmet lctl lalt lsft)) ;; Then, set OS to launch apps via Hyper+letter

  `~ (tap-hold $tt $ht ` S-`)
  \| (tap-hold 0 $ht \ S-\)
)

(defalias ;; extend macros
  mwu (mwheel-up 80 120)
  mwd (mwheel-down 80 120)
  clipb (multi lctl lalt lsft lmet v) ;; Set CopyQ shortcut to match
  scsht prtsc ;; defined as an alias as I may add tap-hold or double-tap to this for alternative SS modes
  grow M-r ;; Cosmic window resizing, in Extend layer as it requires holding+arrows
  shrnk M-S-r ;; Cosmic window resizing, in Extend layer as it requires holding+arrows
)

(defalias
  ;; Base forks. See https://github.com/jtroo/kanata/discussions/1041
  ;; for a potentially better implementation
  q (fork q p (f24)) 
  w (fork w o (f24))
  e (fork e i (f24)) 
  r (fork r u (f24)) 
  t (fork t y (f24))
  a (fork a ' (f24)) 
  s (fork s l (f24)) 
  d (fork d k (f24)) 
  f (fork f j (f24)) 
  g (fork g h (f24))
  z (fork @zalt @/alt (f24))
  gz (fork z / (f24))
  x (fork x . (f24)) 
  c (fork c , (f24)) 
  v (fork v m (f24)) 
  b (fork b n (f24))
  
  ;; NumSym forks
  1 (fork 1 0 (f24)) 
  2 (fork 2 9 (f24)) 
  3 (fork 3 8 (f24)) 
  4 (fork 4 7 (f24)) 
  5 (fork 5 6 (f24))
)

;; Workspace / App launching setup (simple to use, but complex implementation)
;; See https://github.com/jtroo/kanata/discussions/1303
;; And https://github.com/jtroo/kanata/issues/1306

(defvirtualkeys 
 vmet lmet
 meta-trap (layer-while-held meta-trap)
 timer-rls-vmet (macro-release-cancel 350
  (on-press release-vkey vmet))
 timer-rls-vmet-layer (macro-release-cancel 350
  (on-press release-vkey vmet) (on-press release-vkey meta-trap))
)


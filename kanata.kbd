(defsrc
  `         1       2      3        4       5        6      7      8      9      0      -    =    bspc
  tab       q       w      e        r       t        y      u      i      o      p      [    ]    \
  caps      a       s      d        f       g        h      j      k      l      ;      '    ret
  lsft      z       x      c        v       b        n      m      ,      .      /      rsft
  lctl      lmet    lalt   spc      ralt    comp     rmet   rctl

  mfwd mbck ;; mouse
)

(deflayer base
  XX        @n1     @n2    @n3      @n4     @n5      @n6    @n7    @n8    @n9    @n0     XX   XX   bspc
  @tabhyper @q      @w     @e       @r      @t       y      u      i      o      p      ret  XX   XX
  @escext   @a      @s     @d       @f      @g       h      j      k      l      ;      '    ret
  @bspclsft @zalt   @x     @c       @v      @b       n      m      ,      .      @/alt  @delrsft
  XX        lalt    lctl   spc      @nmsm   ralt     XX     XX

  @nmsm f24 ;; mouse
) 

(deflayer numsym
  @f12      @f1     @f2    @f3      @f4     @f5      @f6    @f7    @f8    @f9    @f10   @f11 @f12    _
  _         @`~     S--    -        [       ]        S-[    S-]    S-=    =      @\|    _    _    _
  _         @1      @2     @3       @4      @5       6      7      8      9      0      S-;  _
  _         @!alt   S-8    S-4      S-9     S-0      S-,    S-.    _      _      _      _
  _         _       _      _        _       _        _      _

  _    _
)

(deflayer extend
  _         _       _      _        _       _        _      _      _      _      _      _    _    _
  _         A-f4    XX     @mwu     f2      XX       prev   vold   volu   next   pp     _    _    _
  _         ret     XX     @mwd     lsft    XX       left   down   up     rght   brdn   brup _
  _         lrld    XX     @scsht   @clipb  caps     home   pgdn   pgup   end    _      _
  _         _       _      @lmet    _       _        _      _

  _    _
)

;; Todo: implement good mouse layer accessed via (, .) combo
(deflayer mouse
  _         _       _      _        _       _        _      _      _      _      _      _    _    _
  _         _       _      _        _       _        _      _      _      _      _      _    _    _
  _         _       _      _        _       _        _      _      _      _      _      _    _
  _         _       _      _        _       _        _      _      _      _      _      _
  _         _       _      _        _       _        _      _

  _    _
)

;; Since Windows' Start menu shortcut can't be easily changed we use Kanata for it.
;; Set PowerToys' Run to Win+Spc and avoid the awful Start menu. Tap for M-spc, hold for lmet:
(platform (win winiov2 wintercept)
  (defalias lmet (tap-hold-press 0 $ht M-spc lmet))
)

;; Meta is left alone on Linux/Mac but still must be defined:
(platform (linux macos)
  (defalias lmet lmet)
)

(defvar ;; Generic variables for use throughout config:
  tap-timeout 100
  hold-timeout 150
  tt $tap-timeout
  ht $hold-timeout

  slow-tap-timeout 150
  slow-hold-timeout 275
  stt $slow-tap-timeout
  sht $slow-hold-timeout

  one-shot-timer 350
  ost $one-shot-timer

  delay-fix 15
  df $delay-fix
)

(defalias
  nmsm (layer-while-held numsym)
  escext (tap-hold-press 0 $ht esc (layer-while-held extend))
  togglemouse (switch
          ((layer base)) (layer-switch mouse) break
          ((layer mouse)) (layer-switch base) break
        )

  tabhyper (tap-hold-press 0 $ht tab (multi lctl lalt lsft lmet))
  delrsft (tap-hold-press $tt $ht del rsft)
  bspclsft (fork (tap-hold-press $tt $ht bspc lsft) @delrsft (f24))

  zalt (tap-hold-press 0 $sht z ralt)
  /alt (tap-hold-press 0 $sht / ralt)
  !alt (tap-hold 0 $ht S-1 ralt)
  ?alt (tap-hold 0 $ht S-/ ralt)

  `~ (tap-hold $tt $ht ` S-`)
  \| (tap-hold 0 $ht \ S-\)

  mwu (mwheel-up 80 120)
  mwd (mwheel-down 80 120)
  clipb (multi lctl lalt lsft lmet v) ;; Set CopyQ shortcut to match
  scsht prtsc ;; defined as an alias for future expansion (different SS modes)

  ;; Mirror right half of keyboard onto left half when holding f24 (mouse "Back" button)
  q (fork q p (f24)) w (fork w o (f24)) e (fork e i (f24)) r (fork r u (f24)) t (fork t y (f24))
  a (fork a ' (f24)) s (fork s l (f24)) d (fork d k (f24)) f (fork f j (f24)) g (fork g h (f24))
  z (fork @zalt @/alt (f24)) x (fork x . (f24)) c (fork c , (f24)) v (fork v m (f24)) b (fork b n (f24))
  1 (fork 1 0 (f24)) 2 (fork 2 9 (f24)) 3 (fork 3 8 (f24)) 4 (fork 4 7 (f24)) 5 (fork 5 6 (f24))

  f6 (tap-hold $tt $ht f6 M-f6)
  f7 (tap-hold $tt $ht f7 M-f7)
  f8 (tap-hold $tt $ht f8 M-f8)
  f9 (tap-hold $tt $ht f9 M-f9)
  f10 (tap-hold $tt $ht f10 M-f10)
  f11 (tap-hold $tt $ht f11 M-f11)
  f1 (fork (tap-hold $tt $ht f1 M-f1) @f10 (f24))
  f2 (fork (tap-hold $tt $ht f2 M-f2) @f9 (f24))
  f3 (fork (tap-hold $tt $ht f3 M-f3) @f8 (f24))
  f4 (fork (tap-hold $tt $ht f4 M-f4) @f7 (f24))
  f5 (fork (tap-hold $tt $ht f5 M-f5) @f6 (f24))
  f12 (fork (tap-hold $tt $ht f12 M-f12) @f11 (f24)) ;; f12 is on left half for 6-column boards so needs mirror

  ;; required to use number keys in aliases:
  k1 1 k2 2 k3 3 k4 4 k5 5 k6 6 k7 7 k8 8 k9 9 k0 0 

  n6 (tap-hold $tt $ht @k6 S-6)
  n7 (tap-hold $tt $ht @k7 S-7)
  n8 (tap-hold $tt $ht @k8 S-8)
  n9 (tap-hold $tt $ht @k9 S-9)
  n0 (tap-hold $tt $ht @k0 S-0)

  ;; Fork to mirror KB as above
  n1 (fork (tap-hold $tt $ht @k1 S-1) @n0 (f24))
  n2 (fork (tap-hold $tt $ht @k2 S-2) @n9 (f24))
  n3 (fork (tap-hold $tt $ht @k3 S-3) @n8 (f24))
  n4 (fork (tap-hold $tt $ht @k4 S-4) @n7 (f24))
  n5 (fork (tap-hold $tt $ht @k5 S-5) @n6 (f24))
)
